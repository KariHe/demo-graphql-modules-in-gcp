{
  "name": "ts-gql-modules",
  "version": "1.0.0",
  "description": "Typescript GraphQL with modules",
  "main": "index.js",
  "scripts": {
    "postinstall": "npm run build",
    "start": "node dist/index.js",
    "build": "npm run build:copy && npm run build:js",
    "build:js": "tsc -p . || echo OK",
    "build:copy": "copyfiles src/**/*.graphql dist/",
    "clean": "rm -rf dist",
    "lint": "tslint -p .",
    "watch": "nodemon --exec ts-node index.ts",
    "test": "jest"
  },
  "keywords": [
    "graphql",
    "apollo",
    "typescript",
    "graphql-modules"
  ],
  "author": "kari.heikkinen@nordcloud.com",
  "license": "MIT",
  "engines": {
    "node": "10.x.x"
  },
  "dependencies": {
    "@google-cloud/datastore": "^4.1.4",
    "@graphql-modules/core": "^0.7.9",
    "@koa/cors": "^3.0.0",
    "apollo-server": "^2.25.3",
    "apollo-server-koa": "^2.8.1",
    "copyfiles": "^2.1.1",
    "dataloader": "^1.4.0",
    "dotenv": "^7.0.0",
    "firebase-admin": "^7.4.0",
    "graphql": "^14.4.2",
    "graphql-import": "^0.7.1",
    "koa": "^2.7.0",
    "reflect-metadata": "^0.1.13",
    "typescript": "^3.5.3",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/graphql": "^14.2.3",
    "@types/jest": "^24.0.16",
    "@types/koa": "^2.0.49",
    "@types/uuid": "^3.4.5",
    "inquirer": "^6.5.0",
    "jest": "^24.8.0",
    "nodemon": "^1.19.1",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0"
  }
}
